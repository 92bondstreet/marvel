{
  "name": "marvel-client",
  "version": "0.1.0",
  "description": "Marvel characters client",
  "private": true,
  "dependencies": {
    "a11y-react-emoji": "^1.0.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.0.0",
    "jw-react-pagination": "^1.1.0",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-content-loader": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typeface-roboto": "^0.0.54",
    "use-http": "^0.1.65"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "now-build": "react-scripts build",
    "coveralls": "yarn test --coverage --watchAll=false && cat ./coverage/lcov.info | coveralls",
    "test-ci": "yarn test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "fetch-mock": "^7.3.3",
    "jest-dom": "^3.2.2",
    "node-fetch": "^2.6.0",
    "react-testing-library": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/@uik/**/*.js",
      "!src/index.js",
      "!src/constants.js",
      "!src/serviceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
